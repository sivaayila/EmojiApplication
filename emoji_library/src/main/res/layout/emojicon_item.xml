<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:emojicon="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.anyware.esolutions.emoji_library.Helper.EmojiconTextView
        android:layout_gravity="center"
        android:id="@+id/emojicon_icon"
        android:layout_width="@dimen/emoji_item_layout"
        android:layout_height="@dimen/emoji_item_layout"
        android:textSize="@dimen/emoji_item_text"
        android:textColor="#000000"
        emojicon:emojiconSize="@dimen/emoji_icon_size"
        android:gravity="center" />
</FrameLayout>